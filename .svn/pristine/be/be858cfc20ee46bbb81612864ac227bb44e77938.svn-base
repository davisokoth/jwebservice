##PostreSQL Hibernate Configurations##
hibernate.connection.url=jdbc:postgresql://localhost:5432/26may
hibernate.connection.username=adempiere
hibernate.connection.password=adempiere
hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect
hibernate.connection.driver_class=org.postgresql.Driver
hibernate.hbm2ddl.auto=create_update
hibernate.transaction.auto_close_session=true
hibernate.c3p0.min_size=5
hibernate.c3p0.max_size=20
hibernate.c3p0.timeout=5000
hibernate.c3p0.max_statements=50
hibernate.c3p0.idle_test_period=300
hibernate.c3p0.acquire_increment=2
